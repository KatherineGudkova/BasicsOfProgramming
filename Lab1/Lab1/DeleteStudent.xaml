<Window x:Class="Lab1.DeleteStudent"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab1"
        mc:Ignorable="d"
        Title="DeleteStudent" Height="450" Width="800">
    <Grid x:Name="RemoveRoot" Background="#FFE8F5F4">
        <Label Content="ВИДАЛИТИ ІНФОРМАЦІЮ" Margin="175,20,175,335" FontSize="30" FontWeight="Bold"/>
        <Label Content="ID СТУДЕНТА" Margin="50,160,50,204" FontSize="28" FontWeight="Bold"/>
        <TextBox x:Name="id" TextWrapping="Wrap" Margin="265,155,50,209" FontSize="24"/>
        <Button x:Name="Delele" Content="ВИДАЛИТИ" Margin="50,325,420,50" Background="#FFC9F1C1" BorderBrush="#FF707070" FontSize="20" Click="Delele_Click"/>
        <Button x:Name="Back" Content="ПОВЕРНУТИСЯ" Margin="420,325,50,50" FontSize="20" FontWeight="Normal" Background="#FFE8B3B3" Click="Back_Click"/>
    </Grid>
</Window>
