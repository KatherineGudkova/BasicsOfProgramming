<Window x:Class="Lab4.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab4"
        mc:Ignorable="d"
        Title="Window2" Height="450" Width="650">
    <Grid HorizontalAlignment="Center" Width="650">
        <DataGrid x:Name="Tab2" FontSize="14" VerticalAlignment="Top" Height="340" Margin="10,10,10,0"/>
        <Button x:Name="ToMainWindow" Content="ГОЛОВНА СТОРІНКА" Margin="10,360,335,0" Background="#FFCDEF77" VerticalAlignment="Top" Click="ToMainWindow_Click" Height="35"/>
        <Button x:Name="Exit" Content="ВИХІД" Margin="335,360,10,0" Background="#FFEF6262" VerticalAlignment="Top" Click="Exit_Click" Height="35"/>

    </Grid>
</Window>
