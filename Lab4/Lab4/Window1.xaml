<Window x:Class="Lab4.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab4"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="520">
    <Grid Margin="0,0,400,0">
        <DataGrid x:Name="Tab1" FontSize="14" Margin="10,10,-390,0" VerticalAlignment="Top" Height="340" />
        <Button x:Name="ToMainWindow" Content="ГОЛОВНА СТОРІНКА" Margin="10,360,-130,0" Background="#FFFFFFC5" VerticalAlignment="Top" Click="ToMainWindow_Click" Height="35"/>
        <Button x:Name="Exit" Content="ВИХІД" Margin="270,360,-390,0" Background="#FFEF6262" VerticalAlignment="Top" Click="Exit_Click" Height="35"/>
    </Grid>
</Window>
