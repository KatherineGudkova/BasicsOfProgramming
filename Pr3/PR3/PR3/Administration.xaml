<Window x:Class="PR3.Administration"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PR3"
        mc:Ignorable="d"
        Title="Administration" Height="300" Width="400">
    <Grid Background="#FFE9E9EA" >
        <Button x:Name="ChangePass" Content="ЗМІНИТИ ДАНІ" Margin="35,10,35,0" VerticalAlignment="Top" Background="#FFD4F7AC" FontSize="20" Height="50" Click="ChangePass_Click"/>
        <Button x:Name="UsersView" Content="СПИСОК КОРИСТУВАЧІВ" Margin="35,70,35,0" VerticalAlignment="Top" Background="#FFD4F7AC" FontSize="20" Height="50" Click="UsersView_Click"/>
        <Button x:Name="AddUser" Content="ДОДАТИ КОРИСТУВАЧА" Margin="35,130,35,0" VerticalAlignment="Top" Background="#FFD4F7AC" FontSize="20" Height="50" Click="AddUser_Click"/>
        <Button x:Name="ToMainWindow" Content="ГОЛОВНА СТОРІНКА" Margin="35,190,35,0" VerticalAlignment="Top" Background="#FFEFA4A4" FontSize="20" Height="50" Click="ToMainWindow_Click"/>

    </Grid>
</Window>
